  box-shadow: 0 25px 50px -20px rgba(15, 23, 42, 0.9);
  border: 3px solid rgba(255, 255, 255, 0.6);
}

.appointmentsList {
  display: flex;
  flex-direction: column;
  gap: 1rem;
}

.reportAppointmentCard {
  display: grid;
