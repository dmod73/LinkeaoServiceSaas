  color: #374151;
}

.timeInputs {
  display: flex;
  align-items: center;
  gap: 0.5rem;
}

.timeInputs input {
  padding: 0.5rem;
  border: 1px solid #d1d5db;
  border-radius: 6px;
  font-size: 0.875rem;
}

.timeInputs span {
  color: #6b7280;
  font-weight: 500;
}

.breakRow select {
  padding: 0.5rem;
