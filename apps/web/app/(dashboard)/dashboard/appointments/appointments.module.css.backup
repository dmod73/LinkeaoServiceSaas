  border-radius: 6px;
  font-size: 0.875rem;
  background: white;
}

.breakRow button {
  padding: 0.5rem;
  border: none;
  background: #fee2e2;
  border-radius: 6px;
  cursor: pointer;
  font-size: 1rem;
  transition: all 0.2s;
}

.breakRow button:hover {
  background: #fecaca;
}

/* ===== APPOINTMENT ACTIONS ===== */
.statusActions {
  display: flex;
  gap: 0.5rem;
  margin-top: 0.5rem;
}

.statusActions button {
  padding: 0.5rem 0.75rem;
  border: none;
  background: #f3f4f6;
  border-radius: 6px;
  cursor: pointer;
  font-size: 1rem;
  transition: all 0.2s;
}

.statusActions button:hover {
  background: #e5e7eb;
  transform: scale(1.1);
}
/* ===== RESPONSIVE ===== */
/* Note: Responsive styles moved to component definitions above using mobile-first min-width approach */

