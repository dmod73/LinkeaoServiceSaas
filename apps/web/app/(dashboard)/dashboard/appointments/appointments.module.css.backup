}

/* ===== FORM SECTIONS ===== */
.formSection {
  display: flex;
  flex-direction: column;
  gap: 1rem;
}

.toggleGroup {
  display: flex;
  gap: 0.5rem;
  margin-bottom: 1rem;
}

.formFields {
  display: flex;
  flex-direction: column;
  gap: 1rem;
}

.formFields input,
.formFields textarea {
  padding: 0.75rem;
  border: 1px solid #d1d5db;
  border-radius: 8px;
  font-size: 0.9375rem;
  transition: all 0.2s;
}

.formFields input:focus,
.formFields textarea:focus {
  outline: none;
  border-color: #3b82f6;
  box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.1);
}

/* ===== CLIENT LIST ===== */
.clientList {
  display: flex;
  flex-direction: column;
  gap: 0.5rem;
  max-height: 300px;
  overflow-y: auto;
}

.clientItem,
.clientItemSelected {
  padding: 1rem;
  border: 2px solid #e5e7eb;
  border-radius: 8px;
  cursor: pointer;
  transition: all 0.2s;
  display: flex;
  flex-direction: column;
  gap: 0.25rem;
}

.clientItem:hover {
  border-color: #3b82f6;
  background: #eff6ff;
}

.clientItemSelected {
  border-color: #3b82f6;
  background: #eff6ff;
}

.clientItem strong,
.clientItemSelected strong {
  font-size: 0.9375rem;
  color: #1f2937;
}

.clientItem span,
.clientItemSelected span {
  font-size: 0.8125rem;
  color: #6b7280;
}

/* ===== SERVICE GRID ===== */
.serviceGrid {
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(150px, 1fr));
  gap: 1rem;
}

.serviceCard {
  padding: 1rem;
  border: 2px solid #e5e7eb;
  border-radius: 8px;
  cursor: pointer;
  transition: all 0.2s;
  display: flex;
  flex-direction: column;
  gap: 0.5rem;
  text-align: center;
}

.serviceCard:hover {
  border-color: #3b82f6;
  background: #eff6ff;
  transform: translateY(-2px);
  box-shadow: 0 4px 12px rgba(59, 130, 246, 0.15);
}

.serviceCard strong {
  font-size: 0.9375rem;
  color: #1f2937;
}

.serviceCard span {
  font-size: 0.8125rem;
  color: #6b7280;
}

/* ===== HOUR/BREAK ROWS ===== */
.hourRow,
.breakRow {
  display: flex;
  align-items: center;
  gap: 1rem;
  padding: 0.75rem;
  border-radius: 8px;
  background: #f9fafb;
}

.checkbox {
  display: flex;
  align-items: center;
  gap: 0.5rem;
  cursor: pointer;
  flex: 1;
}

.checkbox input {
  width: 20px;
  height: 20px;
  cursor: pointer;
}

.checkbox span {
